{
  "name": "react-icons-ng-root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.4.10",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "latest",
    "lerna": "^7.1.5",
    "prettier": "^3.0.1"
  },
  "scripts": {
    "lint": "yarn eslint .",
    "format": "prettier --write --ignore-unknown .",
    "format:ci": "prettier --check --ignore-unknown .",
    "version-up": "lerna version",
    "version-snapshot": "lerna version --no-push --no-git-tag-version --yes --preid snapshot prepatch --force-publish",
    "version": "yarn install && git add .",
    "lerna": "lerna",
    "clean": "rm -rfv node_modules && rm -rfv packages/*/node_modules",
    "vercel-build": "cd packages/preview && yarn install && yarn build"
  }
}
